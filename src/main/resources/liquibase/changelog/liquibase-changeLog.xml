   <changeSet id="00000000000000" author="jhipster" dbms="postgresql,oracle">
        <createSequence sequenceName="hibernate_sequence" startValue="1000" incrementBy="1"/>
    </changeSet>
     
    <changeSet author="sebin" id="435682356456791467356892">
    </changeSet>
 </databaseChangeLog>